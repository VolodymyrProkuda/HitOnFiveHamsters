<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="99sp"
        android:layout_height="99sp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="84dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@drawable/angry_hom_empty" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="99sp"
        android:layout_height="99sp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@drawable/angry_hom_empty" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="99sp"
        android:layout_height="99sp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="46dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:srcCompat="@drawable/angry_hom_empty" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="99sp"
        android:layout_height="99sp"
        android:layout_marginStart="48dp"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:srcCompat="@drawable/angry_hom_empty" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="99sp"
        android:layout_height="99sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.211"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.226"
        app:srcCompat="@drawable/angry_hom_empty" />

    <TextView
        android:id="@+id/textViewHitOn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="32dp"
        android:text="Hit On Hamsters!!!"
        android:textColor="#FFC107"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="39dp"
        android:text="GO!!!"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/textViewHitOn"
        app:layout_constraintTop_toBottomOf="@+id/textViewHitOn" />
</androidx.constraintlayout.widget.ConstraintLayout>